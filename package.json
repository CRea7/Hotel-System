{
  "name": "hotel-system",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "12.13.0"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha --exit",
    "start": "node ./dist/bin/www",
    "build": "npm-run-all clean compile:all",
    "compile:models": "babel ./models --out-dir dist/models",
    "compile:routes": "babel ./routes --out-dir dist/routes ",
    "compile:app": "babel app.js --out-dir dist/",
    "compile:copy": "cp -r ./node_modules/ dist/node_modules && cp -r bin/ dist/bin && cp -r views/ dist/views",
    "compile:all": "npm-run-all compile:models compile:routes compile:app compile:copy",
    "clean": "rimraf ./dist && mkdir dist",
    "postinstall": "rimraf ./dist && npm run compile:all"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "dotenv": "^8.2.0",
    "bcrypt": "^3.0.6",
    "jsonwebtoken": "^9.0.0",
    "chai": "^4.2.0",
    "chai-subset": "^1.6.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "cross-env": "^6.0.3",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "lodash": "^4.17.15",
    "mocha": "^6.2.2",
    "mongodb": "^3.3.3",
    "mongodb-uri": "^0.9.7",
    "mongoose": "^5.7.6",
    "morgan": "~1.9.1",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.6.3",
    "@types/express": "^4.17.1",
    "chai": "^4.0.2",
    "cross-env": "^6.0.3",
    "mocha": "^6.2.0",
    "mongodb-memory-server": "^5.2.7",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2"
  }
}
